{
  "name": "@wezz/window-reference-store",
  "version": "0.1.3",
  "description": "A script to manage global references on the window object",
  "type": "module",
  "main": "./dist/window-reference-store.umd.cjs",
  "module": "./dist/window-reference-store.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/window-reference-store.js",
      "require": "./dist/window-reference-store.umd.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "npm run dev",
    "dev": "vite",
    "build": "tsc && vite build",
    "prepare": "npm run build",
    "pack": "npm run build",
    "trypublish": "npm publish --access public || true"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wezz/WindowReferenceStore"
  },
  "author": "Wezz Balk <github@wezz.se> (https://github.com/wezz)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wezz/WindowReferenceStore/issues"
  },
  "homepage": "https://github.com/wezz/WindowReferenceStore",
  "keywords": [
    "helper",
    "window"
  ],
  "devDependencies": {
    "@types/node": "^18.14.2",
    "typescript": "4.9.3",
    "vite": "^4.1.4",
    "vite-plugin-dts": "2.0.2"
  }
}
